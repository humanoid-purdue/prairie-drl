    <mesh name="l_foot_pitch" file="l_foot_pitch.stl" />
    <mesh name="l_foot_roll" file="l_foot_roll.stl" />

    <mesh name="r_hip_pitch" file="l_hip_pitch.stl" scale="1 -1 1" />
